{
  "name": "dabble",
  "author": "Robert Lyall",
  "description": "A sandbox application designed for testing various Electron things",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "license": "MIT",
  "dependencies": {
    "electron-simple-updater": "^1.2.0",
    "lodash": "^4.17.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "scripts": {
    "build": "build",
    "release": "publish",
    "start": "electron ."
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "electron": "^1.6.10",
    "electron-builder": "^18.3.5",
    "electron-builder-squirrel-windows": "^18.5.0",
    "electron-simple-publisher": "^0.4.6",
    "webpack": "^2.6.1"
  },
  "build": {
    "appId": "com.dabble",
    "linux": {
      "category": "Office",
      "synopsis": "Desktop client for Natterly",
      "target": "AppImage"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "publish": "never",
    "productName": "Dabble"
  },
  "updater": {
    "url": "https://dabblereleases.s3.amazonaws.com/updates.json"
  }
}
